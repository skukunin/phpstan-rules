{
    "name": "skukunin/phpstan-rules",
    "description": "Custom PHPStan rules for static analysis",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SKukunin\\PHPStanRules\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SKukunin\\PHPStanRules\\Tests\\": "tests/"
        }
